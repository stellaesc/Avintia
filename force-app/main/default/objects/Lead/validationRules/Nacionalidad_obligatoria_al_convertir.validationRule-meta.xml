<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Nacionalidad_obligatoria_al_convertir</fullName>
    <active>false</active>
    <errorConditionFormula>AND(
IsConverted,
ISBLANK(  Nacionalidad__c )
  )</errorConditionFormula>
    <errorMessage>Antes de convertir el candidato, debe de rellenar la nacionalidad</errorMessage>
</ValidationRule>
