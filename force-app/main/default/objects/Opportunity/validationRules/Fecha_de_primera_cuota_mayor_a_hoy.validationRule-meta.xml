<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Fecha_de_primera_cuota_mayor_a_hoy</fullName>
    <active>true</active>
    <errorConditionFormula>AND(
 ISCHANGED( Fecha_de_primera_cuota__c ),
 Fecha_de_primera_cuota__c &lt; TODAY())</errorConditionFormula>
    <errorMessage>La fecha de primera cuota tiene que ser mayor a HOY.</errorMessage>
</ValidationRule>
