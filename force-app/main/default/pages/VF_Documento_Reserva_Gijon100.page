                            <p style="margin: 0"><apex:outputText value=" {!Promotora.name}" rendered="{!Promotora.name!=null}"/>
                                <apex:outputText value=" __________ " rendered="{!Promotora.name==null}" style="color:black;"/>
                            </p>
                        </td>
                        <td style="width: 350px">
                            <br/><br/><br/><br/><br/><br/><br/>
                            <p style="margin: 0">
                                <apex:variable var="count" value="{!0}" />
                                <apex:repeat value="{!Titulares}" var="Titular">
                                    <apex:outputText value="{!IF (count > 0, ' y; ', ' ')}"/>
                                    <apex:outputText value="Don" rendered="{!IF(Titular.Cliente__r.Sexo__pc == 'Hombre', true, false)}" />
                                    <apex:outputText value="Doña" rendered="{!IF(Titular.Cliente__r.Sexo__pc == 'Mujer', true, false)}" />
                                    <b>
                                        <apex:outputText value=" {!Titular.Cliente__r.name}" rendered="{!Titular.Cliente__r.name!=null}"/>
                                        <apex:outputText value=" __________ " rendered="{!Titular.Cliente__r.name==null}" style="color:black;"/>
                                    </b>
                                    <br/>
                                </apex:repeat>
                            </p>
                        </td>
                    </tr>
                </table>
            </div>
        </div>       
    </body>
</apex:page>