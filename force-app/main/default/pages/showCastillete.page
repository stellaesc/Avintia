                                            <apex:variable var="claseEstado" value="type-11" rendered="{!AND(IF(estado = 'Bloqueado temporalmente',True, False),IF(CONTAINS(ultimoModificadorEstado, usuarioActual),True, False),!IF(oportunidad = null, True, False))}"  />  
