    <formulas>
        <name>Descripcioncompleta</name>
        <dataType>String</dataType>
        <expression>&quot;Entrega a cuenta por la compra de &quot; + {!DescripcionPrincipal}</expression>
    </formulas>
    <formulas>
        <name>DescripLinea</name>
        <dataType>String</dataType>
        <expression>LEFT({!ProductoX.Description}, 150)</expression>
    </formulas>
    <formulas>
        <name>ImporteEntrada</name>
        <dataType>Currency</dataType>
        <expression>IF ({!TipodeReserva}= &quot;Reserva sin IVA&quot;,
{!PorcentajeFirmaContrato}*{!ImporteTotal}/100 ,
({!PorcentajeFirmaContrato}*{!ImporteTotal}/100) -  {!ImporteReserva}
)</expression>
        <scale>2</scale>
    </formulas>
    <formulas>
        <name>ImporteLinea</name>
        <dataType>Currency</dataType>
        <expression>{!ImporteEntrada}* ({!ProductoX.TotalPrice}/{!ImporteTotal})</expression>
        <scale>2</scale>
    </formulas>
    <formulas>
        <name>ImporteParticipacionEntrada</name>
        <dataType>Currency</dataType>
        <expression>{!ImporteEntrada} * {!Participante.Porcentaje_de_participacion__c}</expression>
        <scale>2</scale>
    </formulas>
    <formulas>
        <name>ImporteParticipacionLinea</name>
        <dataType>Currency</dataType>
        <expression>{!ImporteLinea} * {!Participante.Porcentaje_de_participacion__c}</expression>
        <scale>2</scale>
    </formulas>
    <formulas>
        <name>NombreLinea</name>
        <dataType>String</dataType>
        <expression>LEFT({!ProductoX.Description}, 80)</expression>
    </formulas>
    <formulas>
        <name>ReferenciaComplet</name>
        <dataType>String</dataType>
        <expression>{!IdOportunidad} + &quot;-&quot; + {!CodigoSociedad} + &quot;-&quot; + &quot;04&quot;</expression>
    </formulas>
